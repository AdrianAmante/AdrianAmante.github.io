<mjml>

  <mj-head>

    <mj-attributes>
      <mj-text font-family="sans-serif" color="#555" />
      <mj-container background-color="#eee" />
      <mj-section background-color="#fff" />
    </mj-attributes>

  </mj-head>

  <mj-body>
    <mj-container width="600">
      
